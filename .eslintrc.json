{
  "extends": "airbnb", 
  "env":  {
    "browser": true
  }, 
  "rules": {
    "linebreak-style": 0,
    "quote-props": 0,
    "quotes": 0,
    "no-plusplus":0,
    "no-param-reassign":0,
    "class-methods-use-this":0,
    "no-use-before-define":0,
    "consistent-return":0,
    "no-unused-vars":0
  },
  "globals": {
    "TweenMax": true
  }
}